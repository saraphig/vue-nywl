* {
  list-style: none;
  margin: 0;
  padding: 0;
}

.form-group__message {
  color: #f57f6c;
  line-height: 16px;
  display: none;
  font-size: 12px;
  position: absolute;
}

.mt10 {
  margin-top: 10px;
}

.fr {
  float: right;
}

.equipment {
  padding: 20px 60px 0;
}

.equipmentHead {
  height: 50px;
  .title {
    font-size: 30px;
    float: left;
    margin-top: 10px;
  }
}

.equipment_list {
  float: left;
  width: 50%;
  position: relative;
  margin-bottom: 16px;
}

.equipment_list.w100 {
  width: 100%;
}

.equipment_list .sel {
  float: left;
  margin-left 30px
}

.sel:last-child {
  margin-bottom: 0px;
}

.equipment_list1 {
  margin-bottom: 10px;
  font-size: 14px;
  color: #313131;
}

.ml5 {
  margin-left: 5px;
}

.equipment_list2 {
  font-size: 14px;
  color: #313131;
}

.equipment_list2 span {
  padding-right: 95px;
}

.equipment_list2 span:last-child {
  padding: 0;
}

.equipment_input1 {
  width: 220px;
  height: 30px;
  text-indent: 10px;
  line-height: 28px;
  border: 2px solid #b1b4b6;
  border-radius: 4px;
}

.equipment_textarea {
  width: 100%;
  padding: 5px 10px;
  line-height: 18px;
  border: 2px solid #b1b4b6;
  border-radius: 4px;
  box-sizing: border-box;
  font-size 14px
  font-weight 600
}

.equipment_input1.w68 {
  width: 68px;
  margin: 0 16px;
  font-size 15px
}

.equipment_button {
  text-align: right;
}

.equipment_button input {
  width: 120px;
  height: 50px;
  //border: 1px solid #333;
  text-align: center;
  line-height: 38px;
  border-radius: 4px;
  font-family: SourceHanSansCN-Bold;
  font-size: 18px;
  font-weight: normal;
  cursor: pointer;
}

.equipment_button input:nth-child(1) {
  background: #b0b1b4;
  border none
  color #FFFFFF
}

.equipment_button input:nth-child(2) {
  background: #0b82fd;
  color: #fff;
  margin-left: 8px;
  border none
}

.sel {
  position: relative;
  width: 226px;
  margin-bottom: 12px;
}

.sel.w100 {
  width: 100%;
  margin: 0;
}

.sel ol {
  position: absolute;
  width: 100%;
  top: 40px;
  color: #313131;
  font-size: 16px;
  line-height: 30px;
  border: 2px solid #b1b4b6;
  background: #fff;
  box-sizing: border-box;
  text-indent: 10px;
  z-index: 1;
  display: none;
}

.sel li {
  position: relative;
  width: 100%;
  float: left;
}

.sel ol li:hover {
  background: #eee;
  cursor: pointer;
}

.sel .input-wrap {
  position: relative;
  width: 100%;
}

.equipment_list .sel .input-wrap:before {
  content: "偏低";
  display: inline-block;
  width: 50px;
  height: 40px;
  line-height: 40px;
  text-align: center;
  position: absolute;
  left: 0px;
  top: 0px;
  background: #0b82fd;
  color: #fff;
  font-size: 14px;
  border-radius: 5px 0 0 5px;
}

.equipment_lists .sel:nth-child(2) .input-wrap:before {
  content: "偏高";
}

.equipment_lists .sel:nth-child(3) .input-wrap:before {
  content: "过低";
}

.equipment_lists .sel:nth-child(4) .input-wrap:before {
  content: "过高";
}

.equipment_lists .sel:nth-child(5) .input-wrap:before {
  content: "断开";
}

.equipment_list .input-box {
  padding-left: 50px;
}

.equipment_list3 .sel .input-wrap:before {
  display: none
}

.equipment_list3 .input-box {
  padding-left: 0px;
}

.input-wrap-head input {
  height: 50px;
  line-height: 50px;
  background: none;
  border-radius: 4px;
  font-size: 18px;
}

.input-box {
  width: 100%;
  height: 40px;
  text-indent: 10px;
  line-height: 40px;
  border: 2px solid #b1b4b6;
  border-radius: 4px;
  cursor: pointer;
  box-sizing: border-box;
  font-weight: bold;
  color: #313131;
  font-size 18px
}

.sel .iconfont {
  position: absolute;
  right: 5px;
}

.sel .iconfont.icon-paixujiantouxia {
  top: 15px;
}

.sel .iconfont.icon-paixujiantoushang {
  top: 6px;
}

.sel .iconfont.icon-paixujiantouxia.normal {
  top: 8px;
}

:-moz-placeholder {
  color: #b0b1b4;
}

::-moz-placeholder {
  color: #b0b1b4;
}

input:-ms-input-placeholder,
textarea:-ms-input-placeholder {
  color: #b0b1b4;
}

input::-webkit-input-placeholder,
textarea::-webkit-input-placeholder {
  color: #b0b1b4;
}

.searchBox
  position: relative;
  width: 226px;
  float: right;
  margin-right: 30px;
  .seach_input
    width: 205px;
    height: 46px;
    line-height: 46px;
    border-radius: 4px;
    border: #b1b4b6 2px solid;
    background: none;
    font-size: 18px;
    padding-left: 18px;
    font-weight: bold;
  .seach_bott
    position: absolute;
    right: 5px;
    top: 0px;
    width: 50px;
    height: 50px;
    line-height: 50px;
    font-size: 22px;
    color: #b0b1b4;
    font-weight: bold;
    cursor: pointer;
    text-align: center;

input::-ms-clear {
  display: none;
}

@media screen and (max-width: 1199px) {
  .searchBox , .sel{
    width: 200px;
  }
 .searchBox .seach_input {
   width:180px;
 }
}
