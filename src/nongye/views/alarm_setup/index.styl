.input-wrap {
    position relative
    display inline-block
    width: 92.5%;
    height: 36px;
    // margin-right 60px
    .normal {
        right: 15px;
        top: 9px;
        color #999999
        cursor: pointer;
    }
    .iconfont {
        position: absolute;
    }
}
.input-box {
    width: 100%;
    height: 36px;
    text-indent: 10px;
    line-height: 30px;
    border: 1px solid #e3e8ea;
    border-radius: 4px;
    cursor: pointer;
    box-sizing: border-box;
    // font-weight: bold;
    color: #999999;
    font-size 14px
    padding-left 15px
}

.drop_down {
    display none
    z-index 999
    margin 0
    padding 0
}

