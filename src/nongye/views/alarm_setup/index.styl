.input-wrap ul {
    position: absolute;
    width: 100%;
    top 36px
    left 0px
    color: #313131;
    font-size: 14px;
    line-height: 30px;
    border: 1px solid #e3e8ea;
    background: #fff;
    box-sizing: border-box;
    text-indent: 10px;
    z-index: 1;
    display: none;
    margin 0
    padding 0
}

.input-wrap li {
    position: relative;
    width: 98%;
    font-size 14px
    margin 0
    padding 0
    padding-left 15px
    float: left;
    list-style-type none
}

.input-wrap ul li:hover {
    background: #f1f7fa;
    cursor: pointer;
}

.alarm-pretime {
    width: 260px;
    padding: 5px 10px;
    line-height: 18px;
    border: 2px solid #b1b4b6;
    border-radius: 4px;
    box-sizing: border-box;
    font-size 14px
    font-weight 600
    margin-left 30px
}

.alarm-dealIntroduct {
    height 100px
    width: 96%;
    padding: 5px 10px;
    line-height: 18px;
    border: 1px solid #e3e8ea;
    border-radius: 4px;
    box-sizing: border-box;
    font-size 14px
    font-weight 600
    color #666666
    padding-bottom 20px
    // margin-left 30px
}

.hint-pretime {
    position relative    
    display inline-block
    width 16px
    height 16px
    text-align: center;
    border-radius: 8px;
    background #999999
    color #ffffff
    margin-left 20px
}
.hint-content {
    position absolute
    display inline-block
    width 250px
    height 140px
    left 8px
    top 16px
    line-height 24px
    text-align: left;
    border-radius: 4px;
    background #595959
    color #ffffff
    padding 20px
    box-sizing border-box
    word-wrap break-word
    word-break normal
    white-space normal
    color #d5d5d5
    font-size 13px
}
.label-prefix {
    display inline-block
    height 16px
    width 4px
    border-radius: 2px;
    background #41a259
    margin-left 10px
    margin-right 15px
    vertical-align middle
}
.equipment_lists {
     margin-left 30px
}
.form-group__message {
    display none
}