::-ms-clear,::-ms-reveal{display:none};

.main-container {
    height: 100%;
    width: 97.55%;    
    padding: 20px;
    text-align:center;
    background #efff4
}
.content_panel {
    display inline-block
    box-sizing border-box
    width 100%
    // height 700px
    text-align center
    vertical-align middle
    background #ffffff
    border 1px solid #e1e6e9
    padding-bottom 100px
    // margin-left 10px
    // width 83%
    // margin-top:30px;
    .title-equipment {
        display: inline-block;  
        box-sizing: border-box;
        width: 96.31%;
        height: 86px;
        border-bottom 1px solid #f2f2f2
        text-align left
        font-size 19px
        font-weight bold
        line-height 86px
        //margin-left 28px
        vertical-align middle
        
        .equipment_button {
            text-align left   
            // margin-top 20px
            // margin-right 30px
            float right
            
        }
    }
}


.equipment_button input {
    width: 68px;
    height: 36px;
    //border: 1px solid #333;
    text-align: center;
    line-height: 28px;
    box-sizing border-box
    border-radius: 4px;
    font-family: SourceHanSansCN-Bold;
    font-size: 14px;
    font-weight: normal;
    cursor: pointer;
}

.equipment_button input:nth-child(1) {
  background #ffffff
  border 1px solid #41a259
  color #41a259
  text-align: center;
}

.equipment_button input:nth-child(2) {
  background #41a259
  color #ffffff
  margin-left 8px
  text-align center
  border none
}


.equipment_title {
    font-size 16px
    text-align left
    white-space nowrap
    margin-top 30px
    margin-bottom 20px
}
 
.equipment_list {
    display inline-block
    width 100%
    text-align center
    // margin-top 30px
}
.equipment_number {
    display inline-block
    width 96.31%
    text-align left
    white-space nowrap   
    // margin-top 20px
}


.label_number {
    display inline-block
    width 3.5%   
    height 36px
    font-size 13px
    color #999999      
    line-height 36px
    text-align center 
    vertical-align top
    margin 0
}
.number-left {
    width 3%
    text-align left 
}
.number-right {
    width 3%
    text-align right 
}

.equipment_number_input {
    display inline-block
    position relative
    border 1px #ff0000
}

.equipment_input_box {
    display inline-block
    position relative
    width 20%
    // height 36px
    // text-indent 10px
    // line-height 28px
    // font-size 12px
    // border 1px solid #e3e8ea
    // border 1px solid #ff0000
    // border-radius 4px
    margin 0
    .normal-up {
        position: absolute;
        display inline-block
        right: 15px;
        top: 3px;
        color #999999
        // border 1px solid #ff0000
        cursor: pointer;
    }
    .normal-up:hover {
        color #41a259        
    }
    .normal-down {
        position: absolute;
        display inline-block
        right: 15px;
        bottom: 3px;
        color #999999
        // border 1px solid #ff0000
        cursor: pointer;
    }
    .normal-down:hover {
        color #41a259        
    }
    .iconfont {
    }
}

.equipment_input {
    display inline-block
    width 100%
    height 36px
    text-indent 10px
    line-height 28px
    font-size 12px
    border 1px solid #e3e8ea
    border-radius 4px
    margin 0
}
//.ml5,
.equipment_state {
    position relative
    display inline-block
    width 96.31%
    text-align left
    //white-space nowrap   
    // margin-top 20px
}
.equipment_message {
    position relative
    display inline-block
    // margin-bottom 20px
    width 48.08%
    white-space nowrap 
    
    .floatleft {
        float left
    }
    .floatright {
        float right
    }
}
.message-label {
    position relative
    display inline-block
    width 48px
    font-size 14px
    color #999999
}


