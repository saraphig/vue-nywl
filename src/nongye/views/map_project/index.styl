.map-project
    margin: 20px;
    background: #ffffff;
    border: solid 1px #e3e8ea;
    //height: 1162pxï¼›
    .mapProjectTop{
        height :47px;
        margin-top: 39px;
        margin-left: 30px;
        margin-right: 30px;
        border-bottom: 1px solid #e3e8ea;
        box-sizing: border-box;
    }
    .mapProjectTop span{
        font-family: SourceHanSansCN-Medium;
        font-size: 20px;
        font-weight: bold;
        font-stretch: normal;
        line-height: 0px;
        letter-spacing: 0px;
        color: #444444;    
    }
    .basicInformationBox{
        overflow: hidden;
    }
    .informationTitle{
        margin-top: 30px;
        margin-left: 11px;
        margin-bottom: 19px;    
    }
    .informationTitle .bspan1{
        display: inline-block; 
        margin-right: 14px;
        width: 4px;
        height: 16px;
        background-color: #41a259;   
    }
    .informationTitle .bspan2{
        position: absolute;
        display: inline-block;
        //font-family: SourceHanSansCN-Regular;
        font-size: 16px;
        font-weight: normal;
        font-stretch: normal;
        line-height: 16px;
        letter-spacing: 0px;
        color: #444444;
    }
    .informationBoxChild{
        margin-bottom: 10px;
        display: inline-block;
        //float: left;
        box-sizing: border-box;
        height :36px;
        width: 49.8%;    
        padding-left: 30px;
        padding-right: 120px; 
    }
    .informationBoxChild .ibcspan1{
        float: left;
        display: inline-block;
        width: 60px;
        text-align: right ;
        line-height: 36px;
        font-size: 14px;
        font-weight: normal;
        font-stretch: normal;
        letter-spacing: 0px;
        color: #999999;  
        margin-right: 20px;  
    }
    .twochildbox{
        margin-top: 20px!important;    
    }
    .informationBoxChild .ibcspan2{
        //float: left;
        display: block;
        margin-left: 80px;
        line-height: 34px;
        padding-left: 15px;
        width: 100%;
        height: 34px;
        border-radius: 2px;
        border: solid 1px #e3e8ea;
        font-size: 14px;
        color: #444444;
    }
    .informationBoxImg{
        margin-top: 11px;
        padding:0 30px 0 30px;    
    }
    .informationBoxImg .ibcspan3{
        //font-family: SourceHanSansCN-Regular;
        font-size: 14px;
        font-weight: normal;
        font-stretch: normal;
        line-height: 0px;
        letter-spacing: 0px;
        color: #999999;    
    }
    .informationBoxImg .mapimgbox{
        box-sizing: border-box;
        padding: 47px 121px;
        margin-top: 20px;
        //height :600px;
        border: 1px solid #e3e8ea;    
    }
    .informationBoxImg img{
        height :500px;
        width: 100%;    
    }
    .informationbottombox{
        margin-bottom: 60px;    
    }
    .imgspan{
        display inline-block
        height :500px;
        width: 100%; 
        text-align: center;
        line-height: 500px;
        font-size:30px;
        color:#888 
    }