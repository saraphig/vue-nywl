    .main-container {
        display inline-block
        box-sizing border-box
        width 97.55%
        // height 700px
        text-align center
        vertical-align middle
        background #ffffff
        border 1px solid #e1e6e9
        padding-bottom 100px
        margin 20px
        vertical-align middle
        margin-right 0

        .title-container {
        	color:#444444;
        	margin-left: 30px;
            margin-top: -5px;
            position relative
            display: inline-block;  
            box-sizing: border-box;
            width: 100%;
            height: 95px;
            //border-bottom 1px solid #f2f2f2
            text-align left
            font-size 20px
            font-weight bold
            line-height 20px
            //margin-left 28px
            vertical-align middle
            white-space nowrap
            // border 1px solid #ff0000

            .search-box {
                position relative
                display inline-block  
                box-sizing border-box
                width 250px
                height 36px
                margin-top 30px
                margin-left 40px
                padding 0
                // border 1px solid #ff0000
                
                .search-input {
                    position relative
                    display inline-block  
                    box-sizing border-box
                    width 100%
                    height 100%
                    font-size 14px
                    padding-left 16px
                    border-radius: 2px
                    border 1px solid #E3E8EA
                }
                .seach-iconfont {
                    position absolute
                    top 5px
                    right 10px 
                    width 22px
                    height 23px
                    text-align center
                    font-size 17px
                    // font-weight bold
                    color #999999
                    line-height 23px
                    // border 1px solid #ff0000
                }
                
            } 
            .btn-search {
                box-sizing: border-box;
                display: inline-block; 
                width: 68px;
                height: 36px; 
                font-size 14px
                line-height 24px
                color #ffffff
                margin-left 10px
                border 0px
                background #41a259
                border-radius 2px
                cursor pointer
            }
            .btn-adduser {
                position absolute
                box-sizing: border-box;
                display: inline-block;
                right 63px
                top 27px 
                width: 68px;
                height: 36px; 
                font-size 14px
                line-height 24px
                color #41a259
                background #ffffff
                // margin-left 10px
                // margin-top 29px
                border 1px solid #41A259
                border-radius 2px
                cursor pointer
                // float right
            }
        }

        .table-container {
            padding-left: 30px;
    		padding-right: 30px;
            display: inline-block;  
            box-sizing: border-box;
            width: 100%;
            overflow: visible

            .edit-tab{
                position:relative;
                height:100%;
            }
            .edit-show{
                position:absolute;
                width:150px;
                height:36px;
                padding:6px;
                background:#fff;
                right:0px;
                top:-17px;
                // z-index:200;
                -moz-box-shadow:0px 1px 16px #ccc; -webkit-box-shadow:0px 1px 16px #ccc; box-shadow:0px 1px 16px #ccc;
                box-shadow:0px 1px 16px #ccc; -webkit-box-shadow:0px 1px 16px #ccc; box-shadow:0px 1px 16px #ccc;
                li{
                    float:left;
                    width:33%;
                    text-align:center;
                    list-style-type none
                    i{
                        font-size:24px;
                        margin-top 2px
                        display:block;
                    }
                    span{
                        font-size:12px;
                        display:block;
                        color:#999999;
                        margin-top 2px
                        cursor pointer
                    }
                    span:hover {
                        color:#41a259;
                    }
                }
                li:last-child{
                    line-height:36px
                    border-left:1px solid #e5e6e7;
                }
            }
            .showDetailmask{
                position:fixed;
                top:0px;
                left:0px;
                width:100%;
                height:100%;
                z-index:999;
            }
        }
    }
// }